        "                {\"role\": \"system\", \"content\": \"You are a helpful assistant.\"},\n",
        "                {\"role\": \"user\", \"content\": prompt}\n",
        "            ],\n",
        "            max_tokens=150,\n",
        "            temperature=0.7,\n",
        "        )\n",
        "        # Extract and return the assistant's reply\n",
        "        return response['choices'][0]['message']['content'].strip()\n",
        "    except Exception as e:\n",
